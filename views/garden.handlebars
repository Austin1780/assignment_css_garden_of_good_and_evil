
  <div class="main">
    <h1>Hello, world!</h1>

    <form>
      <div class="form-group">
        <input type="radio" name="goodEvil" id="good"/>
        <label for="good">Good</label>
        <input type="radio" name="goodEvil" id="evil"/>
        <label for="evil">Evil</label>
      </div>
      <div>
        <label for="favoritefood">Favorite Food: </label>
        <input type="text" id="favoritefood" placeholder="Favorite Food" />
      </div>
      <div class="form-group">
        <label for="exampleFormControlSelect1">Favorite Color: </label>
        <select class="form-control" id="exampleFormControlSelect1">
        <option>Blue</option>
        <option>Red</option>
        <option>Yellow</option>
        <option>Purple</option>
        <option>Green</option>
        </select>
      </div>
      <label for="fader">Insanity Level</label>
      <input type="range" min="0" max="10" value="5" id="fader"
      	step="1" oninput="outputUpdate(value)">
      <output for="fader" id="volume">5</output>
      <button type="submit" class="btn btn-default">Submit</button>
    </form>

    <script src="https://code.jquery.com/jquery-3.2.1.js" integrity="sha256-DZAnKJ/6XZ9si04Hgrsxu/8s717jcIzLy3oi35EouyE=" crossorigin="anonymous"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js" integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa" crossorigin="anonymous"></script>
    <link href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
    <script>
      function outputUpdate(vol) {
      	document.querySelector('#volume').value = vol;
      }
    </script>
  </div>
